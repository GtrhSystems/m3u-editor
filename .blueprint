created:
    - database/factories/PlaylistFactory.php
    - database/factories/GroupFactory.php
    - database/factories/ChannelFactory.php
    - database/migrations/2024_12_17_230053_create_playlists_table.php
    - database/migrations/2024_12_17_230054_create_groups_table.php
