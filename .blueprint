    - app/Models/Group.php
    - app/Models/Channel.php
models:
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string nullable' }
    Playlist: { name: string, url: string, channels: 'unsignedInteger default:0', synced: 'datetime nullable', relationships: { hasMany: 'Channel, Group' } }
    Group: { name: string, playlist_id: unsignedInteger, relationships: { belongsTo: Playlist } }
    Channel: { name: string, enabled: boolean, channel: 'unsignedInteger nullable', url: string, logo: string, group: string, id: string, lang: string, country: string, playlist_id: unsignedInteger, group_id: unsignedInteger, relationships: { belongsTo: 'Playlist, Group' } }
